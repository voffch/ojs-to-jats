{
  "name":"OJS to JATS",
  "browser_specific_settings": {
    "gecko": {
      "id": "@ojs-to-jats.voffch",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "version":"0.0.2",
  "manifest_version": 3,
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "192": "icon/192.png",
    "512": "icon/512.png"
  },
  "background": {
    "scripts": ["background.js"],
    "type": "module"
  },
  "action": {
    "default_title": "Open page"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
